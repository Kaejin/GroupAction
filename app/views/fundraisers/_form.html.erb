<div class="form-wrapper">
  <div class="container">
    <div class="row">
      <div class="col-xs-12 col-sm-4 col-sm-offset-4">

        <div class="form-box">
          <%= simple_form_for @fundraiser do |f| %>

            <%= f.input :title %>
            <%= f.input :target %>
            <%= f.input :end_date %>

            <%= f.submit %>

          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>


